{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier -w ."
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.17",
    "axios": "^1.13.2",
    "concurrently": "^9.2.1",
    "laravel-vite-plugin": "^1.3.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.93.3",
    "tailwindcss": "^4.1.17",
    "terser": "^5.44.1",
    "vite": "^6.4.1"
  }
}
